




<form id="RegistrationForm">
    <div id="RegistrationErrorView"></div>
    <div class="form-row">

        <div class="form-group col-md-6">

            <label for="exampleInputPassword1">Имя</label>

            <input name="Name" type="text" class="form-control" placeholder="Имя">

        </div>

    </div>

    <div class="form-group col-md-6">

        <label for="exampleInputEmail1">Email адрес</label>

        <input name="Email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">

        <small hidden id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>

    </div>


    <div class="form-row">

        <div class="form-group col-md-6">

            <label for="exampleInputPassword1">Пароль</label>

            <input name="Password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Пароль">

        </div>

        <div class="form-group col-md-6">

            <label for="exampleInputPhone">Мобильный телефон</label>

            <input name="Number" type="tel" class="form-control" id="exampleInputPhone" placeholder="телефон">

            <small hidden id="numberHelp" class="form-text text-muted">пример формата телефона +79905432121</small>
        </div>

    </div>
    <div style="text-align:center;">

        <button type="button" class="btn btn-primary" onclick="SubmitRegistration();">Зарегистрироваться</button>

    </div>

</form>
